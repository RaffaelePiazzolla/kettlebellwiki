<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% routes.forEach(route => { %>
    <% if (!route.path.includes(':') && !route.isFile) { %>
      <url>
         <loc><%= protocol %>://<%= domain %><%= route.path %></loc>
      </url>
    <% } %>
  <% }) %>
</urlset>